concept_deсonstructivism
=>nrel_main_idtf:
	[деконструктивизм] (* <-lang_ru;; *);
	[deсonstructivism] (* <-lang_en;; *);
	[дэканструктывізм] (* <-lang_by;; *);




<- rrel_key_sc_element: 
	...
	(*
	<-sc_explanation;;
	<=nrel_sc_text_translation: 
		...
		(*
		-> rrel_example:
		[Деконструктивизм - стиль в архитектуре, для которого характерны визуальная усложнённость, неожиданные изломанные и нарочито деструктивные формы, а также подчёркнуто агрессивное вторжение в городскую среду.]
			(*
			<- lang_ru;;
			*);;

		*);;

		...
		(*
		-> rrel_example:
		[Deconstructivism - the finished visual appearance of buildings of that style is characterized by unpredictability and controlled chaos.]
			(*
			<- lang_en;;
			*);;

		*);;

		...
		(*
		-> rrel_example:
		[Дэканструктывізм - стыль у архітэктуры, для якога характэрныя візуальная ўскладненасць, нечаканыя пераламаныя і знарок дэструктыўныя формы, а таксама падкрэслена агрэсіўныя ўварванне ў гарадскую сераду.]
			(*
			<- lang_by;;
			*);;

		*);;
	);
