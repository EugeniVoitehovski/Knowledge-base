concept_classicism
=>nrel_main_idtf:
	[архитектура классицизма] (* <-lang_ru;; *);
	[сlassicism] (* <-lang_en;; *);
	[архітэктура класіцызму] (* <-lang_by;; *);
<-concept_architectural_style;

<- rrel_key_sc_element: 
	...
	(*
	<-sc_explanation;;
	<=nrel_sc_text_translation: 
		...
		(*
		-> rrel_example:
		[Архитектура классицизма - стиль архитектуры, главной чертой которого было обращение к формам античного зодчества как к эталону гармонии, простоты, строгости, логической ясности и монументальности.]
			(*
			<- lang_ru;;
			*);;
		*);;
	*);

<- rrel_key_sc_element: 
	...
	(*
	<-sc_explanation;;
	<=nrel_sc_text_translation: 
		...
		(*
		-> rrel_example:
		[Classicism - a style of architecture, the main feature of which was an appeal to the forms of ancient architecture as the standard of harmony, simplicity, rigor, clarity and logic of monumentality.]
			(*
			<- lang_en;;
			*);;
		*);;
	*);

	<- rrel_key_sc_element: 
	...
	(*
	<-sc_explanation;;
	<=nrel_sc_text_translation: 
		...
		(*
		-> rrel_example:
		[Архітэктура класіцызму - стыль архітэктуры, галоўнай рысай якога быў зварот да формаў антычнага дойлідства як да эталону гармоніі, прастаты, строгасці, лагічнай яснасці і манументальнасці.]
			(*
			<- lang_by;;
			*);;

		*);;
	*);;
