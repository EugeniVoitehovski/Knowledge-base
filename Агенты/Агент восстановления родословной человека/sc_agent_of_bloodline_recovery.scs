sc_agent_of_bloodline_relations_recovery
=> nrel_main_idtf:
	[sc-агент восстановления родословной] (* <- lang_ru;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_bloodline_relations_recovery_condition => ..sc_agent_of_bloodline_relations_recovery_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_of_bloodline_relations_recovery
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_bloodline_relations_recovery
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
				agent_of_bloodline_relations_recovery
		};;
	-> sc_agent_of_bloodline_relations_recovery_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_bloodline_relations_recovery_condition
= [*
	question_of_bloodline_relations_recovery _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_bloodline_relations_recovery_result
= [*
	question_of_bloodline_relations_recovery _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;

